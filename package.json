{
    "name": "wheelie-babes-remix",
    "version": "0.0.1",
    "description": "Wheelie Babes - Biking Across America!",
    "author": "Brian Siklinski",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "Theme"
    ],
    "homepage": "https://wheelie-babes.bsik.net",
    "devDependencies": {
        "rtlcss": "^4.1.1",
        "sass": "^1.76.0"
    },
    "rtlcssConfig": {
        "options": {
            "autoRename": false,
            "autoRenameStrict": false,
            "blacklist": {},
            "clean": true,
            "greedy": false,
            "processUrls": false,
            "stringMap": []
        },
        "plugins": [],
        "map": false
    },
    "scripts": {
        "start": "browser-sync start --proxy 'https://wheelie-babes-remix.test/' --files './*.css, ./*.html, ./*.js'",
        "watch:css": "sass --watch --update --style=expanded ./assets/styles:./",
        "compile:css": "sass --no-source-map --style=compressed ./assets/styles:./",
        "compile:rtl": "rtlcss style.css style-rtl.css"
    }
}
