{
    "name": "wheelie-babes-remix",
    "version": "0.0.1",
    "description": "Wheelie Babes - Biking Across America!",
    "author": "Brian Siklinski",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "Theme"
    ],
    "homepage": "https://wheelie-babes.bsik.net",
    "devDependencies": {
        "dotenv": "^16.4.5",
        "dotenv-webpack": "^8.1.0",
        "npm-run-all": "^4.1.5",
        "sass": "^1.76.0",
        "ts-loader": "^9.5.1",
        "typescript": "^5.6.2",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.1.4"
    },
    "rtlcssConfig": {
        "options": {
            "autoRename": false,
            "autoRenameStrict": false,
            "blacklist": {},
            "clean": true,
            "greedy": false,
            "processUrls": false,
            "stringMap": []
        },
        "plugins": [],
        "map": false
    },
    "scripts": {
        "build": "yarn compile:css && webpack",
        "dev": "yarn watch:css & webpack --watch",
        "start": "browser-sync start --proxy 'https://wheelie-babes-remix.test/' --files './static/*.*",
        "watch:css": "sass --watch --update --style=expanded ./assets/styles:./static",
        "watch:ts": "webpack --watch",
        "compile:css": "sass --no-source-map --style=compressed ./assets/styles:./static",
        "compile:ts": "webpack"
    },
    "dependencies": {
        "@types/leaflet": "^1.9.12",
        "@types/leaflet-gpx": "^1.3.7",
        "fuse.js": "^7.0.0"
    }
}
